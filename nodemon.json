{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules",
    "dist"
  ],
  "verbose": true,
  "execMap": {
    "js": "node"
  },
  "runOnChangeOnly": false,
  "watch": [
    "dist/**/*.js",
    "dist/**/*.graphql",
    "dist/**/*.gql"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js,json,graphql",
  "exec": "dist/index.js"
}
